<beans>
    <bean id="miniBean" class="com.wyb.test.spring.simpleSpring.com.wyb.test.spring.ioc.MiniBean" >
        <property name="name" value="11"></property>
        <property name="password" value="22"></property>
    </bean>

    <bean id="miniBean1" class="com.wyb.test.spring.simpleSpring.com.wyb.test.spring.ioc.MiniBean" >
        <property name="name" value="22"></property>
        <property name="password" value="33"></property>
        <property name="miniBean" ref="miniBean"></property>
    </bean>

    <bean id="smallBeanPostProcessor" class="com.wyb.test.spring.ioc.SmallBeanPostProcessor" >
    </bean>

    <bean id="helloService" class="com.wyb.test.spring.aop.com.wyb.test.spring.HelloServiceImpl"/>

    <bean id="logInterceptor" class="com.wyb.test.spring.aop.LogInterceptor"/>

    <bean id="autoProxyCreator" class="com.wyb.test.spring.aop.AspectJAwareAdvisorAutoProxyCreator"/>

    <bean id="helloServiceAspect" class="com.wyb.test.spring.aop.AspectJExpressionPointcutAdvisor">
        <property name="advice" ref="logInterceptor"/>
        <property name="expression" value="execution(* com.wyb.test.spring.HelloService.*(..))"/>
    </bean>
</beans>

